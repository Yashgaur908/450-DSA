//  Question link:- https://practice.geeksforgeeks.org/problems/cyclically-rotate-an-array-by-one2614/1

class Compute {
    
    public void rotate(int arr[], int n)
    {
        int i = n-2;
        int j = n-1;
        while(i >= 0){
            int temp = arr[i];
            arr[i] = arr[j];
            arr[j] = temp;
            i--;
            j--;
        }
    }
}
